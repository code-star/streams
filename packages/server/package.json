{
  "name": "streams-server",
  "version": "0.0.3",
  "description": "Streams Server",
  "main": "index.js",
  "license": "MIT",
  "files": [
    "build"
  ],
  "keywords": [
    "streams-server"
  ],
  "scripts": {
    "start:local": "nodemon --watch src/index.js --exec \"npm run serve\"",
    "start": "npm run serve",
    "serve": "npm run build && node build/index.js",
    "build": "babel src -d build",
    "release": "npm publish",
    "uninstall-packages": "rm -rf node_modules package-lock.json",
    "install-packages": "npm i",
    "reinstall-packages": "npm-run-all uninstall-packages install-packages",
    "uninstall-primer": "npm un -S primer",
    "install-primer": "npm i -S primer",
    "reinstall-primer": "npm-run-all uninstall-primer install-primer",
    "test": "echo \"No test specified\" && exit 0",
    "lint": "npm-run-all lint:scss lint:js",
    "lint:scss": "stylelint src/**/*.scss --fix",
    "lint:js": "eslint src/**/*.js --fix",
    "lint:staged": "lint-staged",
    "ci": "npm-run-all lint build test"
  },
  "dependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "nodemon": "^1.18.8",
    "npm-run-all": "^4.1.5",
    "uuid": "^3.3.2"
  }
}
