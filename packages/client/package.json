{
  "name": "streams-client",
  "version": "0.0.2",
  "description": "Streams Client",
  "main": "index.js",
  "license": "MIT",
  "files": [
    "build"
  ],
  "keywords": [
    "streams-client"
  ],
  "scripts": {
    "start": "nodemon --watch webpack.config.js --watch webpack.parts.js --exec \"webpack-dev-server --env development\"",
    "serve": "echo \"No serve specified\" && exit 0",
    "build": "webpack --env production",
    "release": "npm publish",
    "uninstall-packages": "rm -rf node_modules package-lock.json",
    "install-packages": "npm i",
    "reinstall-packages": "npm-run-all uninstall-packages install-packages",
    "uninstall-primer": "npm un -S primer",
    "install-primer": "npm i -S primer",
    "reinstall-primer": "npm-run-all uninstall-primer install-primer",
    "test": "echo \"No test specified\" && exit 0",
    "lint": "npm-run-all lint:scss lint:js",
    "lint:scss": "stylelint src/**/*.scss --fix",
    "lint:js": "eslint src/**/*.js --fix",
    "lint:staged": "lint-staged",
    "ci": "npm-run-all lint build test"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^1.0.0",
    "css-loader": "^2.0.0",
    "dotenv-webpack": "^1.5.7",
    "git-revision-webpack-plugin": "^3.0.3",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.7",
    "npm-run-all": "^4.1.5",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^3.0.0",
    "prop-types": "^15.6.2",
    "react-hot-loader": "^4.3.12",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4",
    "yeticss": "^7.4.1"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "google-map-react": "^1.1.1",
    "leaflet": "^1.3.4",
    "primer": "^10.10.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-leaflet": "^2.1.2",
    "react-router-dom": "^4.3.1"
  }
}
